/* components/dialog/dialog.wxss */
.my-dialog {
  opacity: 1;
}
.my-dialog .my-mask {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.6);
  z-index: 1000;
}
.my-dialog .my-container {
  position: fixed;
  width: 65%;
  max-width: 300px;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  background-color: #fff;
  text-align: center;
  border-radius: 15rpx;
  padding:0rpx 46rpx;
  overflow: hidden;
  z-index: 5000;
}
.my-dialog .my-container .title {
  display: inline-block;
  width: 100%;
  margin: 20rpx 0 20rpx 0;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  font-weight: 400;
  font-size: 28rpx;
  color: #303133;
}
.my-dialog .my-container .content {
  margin: 10rpx 0 20rpx;
  display: inline-block;
  width: 100%;
  line-height: 50rpx;
  text-align: left;
  font-size: 26rpx
}
.my-dialog .my-container .btn-row {
  padding: 10rpx 25rpx 0;
  display: inline-block;
  width: 100%;
  height: 100rpx;
  line-height: 68rpx;
  font-size: 18px;
}
.my-dialog .my-container .btn-row .confirm ,.my-dialog .my-container .btn-row .cancel{
  float: left;
  width: 170rpx;
  height: 60rpx;
  border: 1rpx solid #7b6e5b;
  color: #7b6e5b;
  border-radius: 22rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 22rpx;
  margin-left:28rpx
}
.my-dialog .my-container .btn-row  .confirm {
  color: #ffffff;
  background: #7b6e5b;
}
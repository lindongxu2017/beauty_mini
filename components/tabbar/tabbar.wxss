/* components/tabbar/tabbar.wxss */
.icon{
  width:54rpx;
  height: 54rpx;
}
.tabBar{
  width:100%;
  position: fixed;
  bottom:0;
  padding:10rpx;
  margin-left:-4rpx;
  border-top:1rpx solid #f2f2f2;
  background: #fff;
  font-size:24rpx;
  color:#8A8A8A;
  display: flex;
  justify-content: space-between;
  box-shadow: 3rpx 3rpx 3rpx 3rpx #aaa; 
  z-index: 99;
}
 .tabBar-item{
  width:50%;
  text-align: center;
  overflow: hidden;
}
/*当前字体颜色*/
.tabBartext{
  color:#a0856a;
}
.navigator-hover{
  background-color: rgba(0, 0, 0, 0);
}
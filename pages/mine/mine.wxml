<!--pages/mine/mine.wxml-->
<header header='{{header}}'></header>
<view>
  <block wx:if="111">
    <view class="info">
      <view class="imgBox">
        <image wx:if="{{imgsrc}}" src="../../utils/img/63JsEE6zTsh7aXYN.jpg"></image>
        <image wx:else src="../../utils/img/yonghumorentouxiang.png"></image>
      </view>
      <view class="personalInfo">
        <view class="infoBox">
          <view class="nameBox">爱拔</view>
        </view>
        <view class="personalTel">
          <text wx:if="{{quotation}}"></text>
          <text wx:else>很懒什么也没有留下~</text>
        </view>
      </view>
      <view class="iconfont iconshezhi"></view>
    </view>
    <view class="message">
      <view class="zhanghao">账号：{{zhanghao}}</view>
      <view class="yue">余额：<text>￥{{yue}}.00</text></view>
      <view class="depositBtn" bindtap="depositBtn">去充值</view>
    </view>
    <view class="gap"></view>
  </block>
  <block wx:else>
    <view class="noSIgn">
        <image src="../../utils/img/yonghumorentouxiang.png"></image>
        <view class="statusTip">您还没有登录哦</view>
        <view class="signBTn" bindtap="promiseLogin">授权登录</view>
    </view>
  </block>

</view>

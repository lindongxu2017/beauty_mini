/* Steven */
.ad-wrapper {
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	display: flex;
	align-items: center;
	justify-content: center;
	background-color: rgba(0,0,0,0.607843);
}
.fade-in {
	width: 64.44%;
	animation: fade-in 0.5s ease;
}

@keyframes fade-in {
	from{
		opacity: 0;
		transform: scale(0);
	}
	to{
		opacity: 1;
		transform: scale(1);
	}
}
.swiper-wrapper {
	height: 48vw;
}
.swiper-img {
	width: 100%;
	height: 100%;
}
.grid-wrapper {
	height: 260rpx;
	box-sizing: border-box;
	padding: 40rpx 0;
	font-size: 0;
	background-color: #fff;
}
.grid-item {
	display: inline-block;
	width: 25%;
	text-align: center;
}
.gird-item-img {
	width: 16.67vw;
	height: 16.67vw;
	margin-bottom: 20rpx;
}
.grid-item-text {
	font-size: 28rpx;
	line-height: 40rpx;
}

.special-item-wrapper {
	padding: 33.33vw 20rpx 10rpx;
	background-size: 100% auto;
	background-repeat: no-repeat;
}
.special-content {
	background-color: #fff;
	padding: 0 40rpx;
	border-radius: 10rpx;
	margin-bottom: 20rpx;
}
.special-content .items {
	padding-bottom: 20rpx;
	margin-bottom: 30rpx;
	border-bottom: 1px dashed #999;
}
.special-item-wrapper .items:last-child {
	border-bottom: 0;
	margin-bottom: 0;
}
.special-content .title-text {
	padding: 60rpx 0 40rpx;
	text-align: center;
}
.special-content .title-text .subtitle {
	font-size: 24rpx;
	color: #999;
	margin-top: 10rpx;
}
.title-time {
	display: flex;
	justify-content: space-between;
	align-items: center;
	margin-bottom: 5rpx;
}
.title-time .title {
	flex: 1;
	padding-right: 30rpx;
	width: 0;
}
.title-time .title text {
	display: block;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}
.title-time .time {
	font-size: 24rpx;
	color: #999;
}
.special-content .items .subtitle {
	font-size: 24rpx;
	color: #999;
	margin-bottom: 20rpx;
}
.price-order {
	display: flex;
	justify-content: space-between;
	align-items: center;
}
.items-price {
	color: #FF9900;
}
.items-price .unit, .items-price .point {
	font-size: 28rpx;
}
.items-price .text {
	font-size: 40rpx;
}
.appointment-btn {
	font-size: 26rpx;
	width: 180rpx;
	height: 70rpx;
	text-align: center;
	line-height: 70rpx;
	background: #A0856A;
	border-radius: 70rpx;
	color: #fff;
}

.product-wrapper {
	background-color: #fff;
	border-radius: 10rpx;
	padding-bottom: 30rpx;
}
.product-wrapper .title-wrapper {
	padding: 60rpx 30rpx 40rpx;
	text-align: center;
}
.title-wrapper .title {
	margin-bottom: 10rpx;
}
.title-wrapper .subtitle {
	font-size: 24rpx;
	color: #999;
}
.product-wrapper .list-wrapper {
	padding: 0 10rpx;
	font-size: 0;
}
.product-wrapper .list-wrapper .items {
	display: inline-block;
	width: 33.3333%;
	box-sizing: border-box;
	padding: 0 10rpx;
	text-align: center;
	margin-bottom: 20rpx;
}
.product-img {
	width: 100%;
	height: 24.411764vw;
}
.product-text {
	font-size: 28rpx;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.product-price {
	font-size: 24rpx;
	color: #FF6600;
}
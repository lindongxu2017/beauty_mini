/* pages/cardcase/cardcase.wxss */
.container{
  height: 100%
}
.top-nav{
  height: 100rpx;
  background: rgb(223, 176, 176);
}
.scroll-wrap{
  line-height: 100rpx;
  display:flex;
  text-align: center;
  flex:1;
}
.scroll-wrap view{
  text-align: center;
  width: 23%;
  float: left;
  color: #989898;
  font-size: 28rpx;
  margin:0 5%;
  box-sizing: border-box;
}
.scroll-wrap .active{
  color: #000;
  font-size: 30rpx;
  height: 100rpx;
  border-bottom: 6rpx solid #7b6e5b;
}
.scroll-wrap text:last-child{
  width: 24%
}
.normal{
  width: 100%;
  background: #f1f1ef;
  min-height:1120rpx;
}
.not{
  background: url(data:image/png;base64,) no-repeat;
  line-height: 1120rpx;
  text-align: center;
  background-size: 30%;
  background-position: 50% 30%;
}